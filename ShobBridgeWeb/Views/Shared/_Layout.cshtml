<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
    <link href="~/css/chosen.css" rel="stylesheet" />
    <link href="~/css/pms.css" rel="stylesheet" />
    <link href="~/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/css/dataTables.responsive.css" rel="stylesheet" />
    <link href="~/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/theme-one.css" rel="stylesheet" />
    <link href="~/css/other-page.css" rel="stylesheet" />
    <link href="~/css/theme-two.css" rel="stylesheet" />
    <link href="~/css/media.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <script src="~/js/styleswitcher.js"></script>


</head>
<body>
    <div class="wrapper">

        @Html.Partial("_Header")
        <!--menu-accordian start-->
        @Html.Partial("_SideMenu")
        <!--menu-accordian end-->
        <!-- InstanceBeginEditable name="page-wrapper" -->
        <!--page-wrapper start-->
        <section class="page-wrapper">
            @RenderBody()
            @RenderSection("Content", required: false)

        </section>
        <!--page-wrapper end-->
        <!-- InstanceEndEditable -->
        <!--footer start-->
        @Html.Partial("_Footer")

        <!--footer end-->
    </div>
    @RenderSection("scripts", required: false)

    <script src="~/js/jquery-1.11.2.min.js"></script>
    <script src="~/js/modernizr-latest.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.nicescroll.min.js"></script>
    <script src="~/js/moment.min.js"></script>
    <script src="~/js/bootstrap-datetimepicker.min.js"></script>
    <script src="~/js/jquery.dataTables.min.js"></script>
    <script src="~/js/dataTables.responsive.min.js"></script>
    <script src="~/js/chosen.jquery.min.js"></script>
    <script src="~/js/common.js"></script>
    @*<script src="~/js/jquery.validate.js"></script>
        <script src="~/js/jquery.validate.unobtrusive.js"></script>*@
</body>
</html>
